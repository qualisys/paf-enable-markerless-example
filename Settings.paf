Project ID: PAF ENABLE Markerless Example
Package Information:
  Name: ENABLE Markerless Example
  Version: 1.0.0
  Required QTM version: 2023.1.7900
  Encoding bytes: €€€€€

Root type: Subject
Default fields: [Creation date, Creation time]

Date format: Iso
Date separator: Dash
Filename filter: \.(pdf|r3w|cmo|docx)$

Types:
  Subject:
    Participant:
      Fields: [ID, Name, Sex, DOB, Height, Weight]
      Children: [Session]
      Directory pattern: $Name$_$ID$

  Session:
    Markerless Session:
      Fields: [Session ID, Test condition, Operator]
      Measurements: [Dynamic]
      Analyses: [ENABLE Processing]
      Display order: [Session ID, Test condition, Operator, Comments]
      Directory pattern: $Creation date$_Markerless

Measurements:
  Fields:
    Measurement length: 10
    Minimum count: 1
    Maximum count: 1
    Count: 1
  Dynamic:
    Maximum count: 10
    Count: 3
    Measurement type: Dynamic

Analyses:
  ENABLE Processing:  
    Type: External program
    Program display name: ENABLE
    Export session: Yes
    Export measurements: [c3d,xml settings]
    Arguments: [--input-dir, $WorkingDirectory$, --calib, qualisys_ex]
  
Fields:
  ID:
    Type: String
    Inherit: Connect
  Name:
    Type: String
    Inherit: Connect
    Force: Yes
  DOB:
    Type: Date
    Inherit: Connect
  Creation date:
    Type: Date
    Readonly: Yes
  Creation time:
    Type: TimeOfDay
    Readonly: Yes
  Session ID:
    Type: String
    Inherit: Connect
  Operator:
    Type: String
    Inherit: Copy
  Height:
    Type: Float
    Quantity: Length
    Unit: centimeters
    Decimals: 0
    Inherit: Connect
    Max: 250
    Min: 50
  Weight:
    Type: Float
    Quantity: Mass
    Unit: kilograms
    Decimals: 1
    Inherit: Copy
    Max: 250
    Min: 0
  Sex:
    Type: Enum
    Values: [Unspecified, Female, Male, Other]
    Inherit: Connect
    Default: Unspecified
  Measurement type:
    Type: String
  Test condition:
    Type: String

Columns:
  Default column definitions:
    ID:
      Width: 200
      Fields:
        Default: Filename
        Participant: ID
    Name:
      Width: 150
      Fields: 
        Participant: Name
    Created:
      Width: 65
      Field: Creation date
    Comments:
      Field: Comments
      Width: 100
